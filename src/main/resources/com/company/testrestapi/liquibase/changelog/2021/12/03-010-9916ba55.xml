<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1" author="TestRestAPI">
        <dropColumn columnName="Name" tableName="KnowledgePoints"/>
    </changeSet>
    <changeSet id="2" author="TestRestAPI">
        <addColumn tableName="KnowledgePoints">
            <column name="Name" type="VARCHAR(255)"/>
        </addColumn>
    </changeSet>
    <changeSet id="3" author="TestRestAPI">
        <dropNotNullConstraint columnDataType="varchar(255)" columnName="Name" tableName="KnowledgePoints"/>
    </changeSet>
</databaseChangeLog>